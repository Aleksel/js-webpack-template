{
  "name": "easy-note",
  "version": "0.1.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "run-electron .",
    "start-e": "cross-env START_HOT=1 yarn start",
    "win": "cross-env NODE_ENV=production build --win --x64",
    "linux": "build --linux",
    "mac": "build --mac"
  },
  "build": {
    "productName": "Easy-Note",
    "appId": "omatic.app.easy.note",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/",
      "main.js",
      "package.json"
    ],
    "win": {
      "asar": false,
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "maintainer": "Andrey Savchuk <andrej.savchuk@gmail.com>",
      "target": [
        "deb",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "build"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-async-generator-functions": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "^1.0.1",
    "debug": "^4.1.0",
    "electron": "^4.0.0-beta.6",
    "electron-builder": "^20.36.2",
    "electron-reload": "^1.3.0",
    "electron-timber": "^0.5.1",
    "electron-util": "^0.10.2",
    "electron-webpack": "^2.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "node-static": "^0.7.11",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "react-dev-utils": "^6.1.1",
    "react-hot-loader": "^4.3.12",
    "run-electron": "^0.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electron-json-storage": "^4.1.4",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-intl": "^2.7.2",
    "react-router-dom": "^4.3.1",
    "store": "^2.0.12"
  }
}
